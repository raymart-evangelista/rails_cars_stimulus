<%= form_with model: car do |form| %>
  <div>
    <%= form.label :name %>
    <br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :price %>
    <br>
    <%= form.text_field :price %>
  </div>

  <br>

  <%= render "variants/form", car: car %>

  <br>

  <div>
    <%= form.submit %>
  </div>
<% end %>

